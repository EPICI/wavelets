<components:WindowManager.LinkedTablePane
	xmlns:bxml="http://pivot.apache.org/bxml"
	xmlns:components="ui"
	xmlns="org.apache.pivot.wtk">
	<columns>
		<TablePane.Column width="-1"/>
		<TablePane.Column width="-1"/>
		<TablePane.Column width="1*"/>
	</columns>
	<TablePane.Row>
		<PushButton bxml:id="remove" buttonData="Close"/>
		<PushButton bxml:id="open" buttonData="Open"/>
		<Label bxml:id="label"/>
	</TablePane.Row>
</components:WindowManager.LinkedTablePane>